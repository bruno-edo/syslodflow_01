     -->

    


    <!-- http://ldwpo.aksw.org/terms/1.0/activity -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/activity">
        <rdfs:label xml:lang="en">activity</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the Activities of a Process. As restrictions, one Process has one or more Activities and one Activity is related to one or more Processes.</rdfs:comment>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Activity"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/Process"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/contributor -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/contributor">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">contributor</rdfs:label>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://purl.org/dc/terms/contributor&gt;</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://purl.org/net/opmv/ns#wasControlledBy&gt;</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://purl.org/net/opmv/ns#wasPerformedBy&gt;</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://usefulinc.com/ns/doap#developer&gt;</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://www.w3.org/ns/prov#wasAssociatedWith&gt;</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://xmlns.com/foaf/0.1/maker&gt;</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Object property that relates a Person to the Steps. As restrictions, one Step can be performed by one Person and one Person can be related to
one or more Steps.</rdfs:comment>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Person"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/Step"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/creator -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/creator">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">creator</rdfs:label>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://purl.org/dc/terms/creator&gt;</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://xmlns.com/foaf/0.1/maker&gt;</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Object property that points to a Person who created an LDWProject. As restrictions, one LDWProject is created by one or more Person and one Person can be related to one or more LDWProjects.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWProject"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Person"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/exception -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/exception">
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Exception"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWStep"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/firstLdwStep -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/firstLdwStep">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">firstLdwStep</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the initial LDWStep of an LDWorkflow. As restrictions, one LDWorkflow has one LDWStep as the firstLdwStep and one LDWStep can be the firstLdwStep in one or more LDWorkflows.</rdfs:comment>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWStep"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWorkflow"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/firstLdwStepExecution -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/firstLdwStepExecution">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">firstLdwStepExecution</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the initial LDWStep-Execution of an LDWorkflowExecution. As restrictions, one LDWorkflowExecution has one LDWStepExecution as firstLdwStepExecution and one LDWStepExecution is the firstLdwStepExecution of a single LDWorkflowExecution.</rdfs:comment>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWStepExecution"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWorkflowExecution"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/format -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/format">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">fileFormat</rdfs:label>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://purl.org/dc/terms/format&gt;</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Object property that points to the Format of a Dataset. As restrictions, one Format is related to one or more Datasets and a Dataset is related to a single Format.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/Dataset"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Format"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/homepage -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/homepage">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">homepage</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the Homepage of a LDWProject. As restrictions, one Homepage is related to one or more LDWProjects and a LDWProject is related to a single Homepage</rdfs:comment>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Homepage"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWProject"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/inputDataset -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/inputDataset">
        <rdfs:label xml:lang="en">inputDataset</rdfs:label>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://purl.org/net/opmv/ns#used&gt;</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://www.w3.org/ns/prov#used&gt;</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http:/purl.org/spar/pwo#needs&gt;</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Object property that points to the Datasets used as input by an LDWStep. As restrictions, one LDWStep has one or more Datasets as input and one Dataset can be related as input to one or more LDWSteps.</rdfs:comment>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Dataset"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWStep"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/ldWorkflow -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/ldWorkflow">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">ldWorkflow</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the LDWorkflows related to an LDWProject. As restrictions, one LDWProject has one or more LDWorkflows and one LDWorkflow is related to a single LDWProject.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWProject"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWorkflow"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/ldWorkflowExecution -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/ldWorkflowExecution">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string"> ldWorkflowExecution</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the LDWorkflowExecutions related to an LDWorklow. As restrictions, one LDWorkflow has one or more LDWorkflowExecutions and one LDWorkflowExecution is related to a single LDWorkflow.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWorkflow"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWorkflowExecution"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/ldwStep -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/ldwStep">
        <rdfs:label rdf:datatype="&xsd;string">ldwStep</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the LDWSteps related to an LDWorklow. As restrictions, one LDWorkflow has one or more LDWSteps and one LDWStep is related to one or more LDWorkflows.</rdfs:comment>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWStep"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWorkflow"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/ldwStepExecution -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/ldwStepExecution">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">ldwStepExecution</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the LDWStepExecutions related to an LDWorklowExecution and an LDWStep. As restrictions, one LDWStepExecution is related to a single LDWStep and to a single LDWorkflowExecution, and both LDWorkflowExecution and LDWStep are related to one or more LDWStepExecution.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWStep"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWStepExecution"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWorkflowExecution"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/license -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/license">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">license</rdfs:label>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://purl.org/dc/terms/rights&gt;
</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Object property that points to the type of License of a Dataset. As restrictions, one Dataset has one License and one License is related to one or more Datasets. </rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/Dataset"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/License"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/location -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/location">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">location</rdfs:label>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://www.w3.org/ns/prov#atLocation&gt;</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Object property that points to the physical location of an Artifact, which could be an URL or local filesystem path. As restrictions, one Artifact has one Location and one Location is related to one or more Artifacts.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/Artifact"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Location"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/message -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/message">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">message</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the Message generated as feedback when a LDWStepExecution is executed. As restrictions, one LDWStepExecution has one Message and one Message is related to one LDWStepExecution.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWStepExecution"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Message"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/nextStep -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/nextStep">
        <rdf:type rdf:resource="&owl;AsymmetricProperty"/>
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdfs:label xml:lang="en">nextStep</rdfs:label>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http:/purl.org/spar/pwo#hasNextStep&gt;</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Object property that points to the next Step into a linked list of Steps. It has previousStep as an inverse property. As restriction, one Step has only one next Step and is an irreflexive and asymmetric object property.
</rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/Step"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Step"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/outputDataset -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/outputDataset">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">outputDataset</rdfs:label>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http:/purl.org/spar/pwo#produces&gt;</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Object property that points to the Datasets to be transformed in an LDWStep. As restrictions, one LDWStep has one or more Dataset as output and one Dataset can be related as output to one or more LDWSteps.</rdfs:comment>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Dataset"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWStep"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/postcondition -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/postcondition">
        <rdfs:label rdf:datatype="&xsd;string">postcondition</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the Condition to achieve after an LDWorkflow is executed by an LDWorkflowExecution. As restrictions, one LDWorkflow has one or more Conditions as postcondition and one Condition can be related as postcondiction to one or more LDWorkflows.</rdfs:comment>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Condition"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWorkflow"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/precondition -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/precondition">
        <rdfs:label rdf:datatype="&xsd;string">precondition</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the Condition to be presented before an LDWorkflow is executed by an LDWorkflowExecution. As restrictions, one LDWorkflow has one or more Conditions as precondition and one Condition can be related as recondiction to one or more LDWorkflows.</rdfs:comment>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Condition"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWorkflow"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/previousStep -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/previousStep">
        <rdf:type rdf:resource="&owl;AsymmetricProperty"/>
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdfs:label xml:lang="en">previousStep</rdfs:label>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://purl.org/net/opmv/ns#wasTriggeredBy&gt;</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http:/purl.org/spar/pwo#hasPreviousStep&gt;</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Object property that points to the previous Step into a linked list of Steps. It has nextStep as an inverse property. As restriction, one Step has only one previous Step and is an irreflexive and asymmetric object property.</rdfs:comment>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Step"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/Step"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/report -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/report">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">report</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the Report generated as feedback when an LDWorkflowExecution is executed. It groups all Messages and Statuses from the related LDWStepExecution. As restriction, one LDWorkflowExecution has only one Report and one Report is related to only one LDWorkflowExecution.
</rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWorkflowExecution"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Report"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/status -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/status">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">status</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the Status of executing a LDWStepExecution. As restriction, one LDWStepExecution has only one Status and one Status is related to one or more LDWStepExecution.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWStepExecution"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Status"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/task -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/task">
        <rdfs:label rdf:datatype="&xsd;string">task</rdfs:label>
        <rdfs:comment xml:lang="en">Object property that points to the Task of an LDWStep or Stage. As restrictions, one Task is related one or more Stages and one or more LDWSteps, and one LDWStep and one Stage are related to one or more Tasks.
</rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/Activity"/>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWStep"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Task"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/tool -->

    <owl:ObjectProperty rdf:about="http://ldwpo.aksw.org/terms/1.0/tool">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">tool</rdfs:label>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http://purl.org/net/opmv/ns#used&gt;</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&rdfs;ch_resource">&lt;http:/purl.org/spar/pwo#needs&gt;</rdfs:seeAlso>
        <rdfs:comment xml:lang="en"> Object property that points to the Tool employed in a LDWStep. As restrictions, one Step can employ one Tool and one Tool is related to one or more LDWSteps.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ldwpo.aksw.org/terms/1.0/LDWStep"/>
        <rdfs:range rdf:resource="http://ldwpo.aksw.org/terms/1.0/Tool"/>
    </owl:ObjectProperty>
    


    <!-- http://ldwpo.aksw.org/terms/1.0/toolConfiguration -->

